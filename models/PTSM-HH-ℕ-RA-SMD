// Socket on PCB
if(part)
{
	for(n=[0:N-1])b(-(N-1)*1.25+n*2.5,5.35,0,1.2,3.2,0.5); // Pins
	b(-8.25,1.15,0,2.2,5.6,0.2); // Tab
	b(8.25,1.15,0,2.2,5.6,0.2); // Tab
	hull()
	{
		b(0,0.9,0,1.7+N*2.5,7.5,4);
		b(0,0.9,0,1.7+N*2.5-2,7.5,5);
	}
	b(0,-7.5/2-2.85,0,0.5+N*2.5,7.5,5);
}
if(hole)
{ // Plug
	b(0,-7.5/2-2.85,0,0.5+N*2.5,7.5,5);
}
