// 1.6x1.5mm LED
if(part)
{
        b(0,0,0,1.6+0.2,1.5+0.2,.6+0.2);
}
if(hole)
{
        hull()
        {
                b(0,0,.6+0.2,1.6+0.2,1.5+0.2,1);
                translate([0,0,height-0.5])cylinder(d=1.001,h=0.001,$fn=16);
        }
}
if(block)
{
        hull()
        {
                b(0,0,.6+0.2,3.6+0.2,3.6+0.2,1);
                translate([0,0,height-0.5])cylinder(d=2,h=1,$fn=16);
        }
}
