// 2x2mm LED
if(part)
{
        b(0,0,0,2.0+0.2,2.0+0.2,0.65+0.2);
}
if(hole)
{
        hull()
        {
                b(0,0,.6+0.2,2.0+0.2,2.0+0.2,1);
                translate([0,0,height])cylinder(d=1.001,h=0.001,$fn=16);
        }
}
if(block)
{
        hull()
        {
                b(0,0,.65+0.2,2.8,2.8,1);
                translate([0,0,height])cylinder(d=2,h=1,$fn=16);
        }
}
